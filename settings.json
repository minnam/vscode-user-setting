{
  "window.zoomLevel": -1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.tabSize": 2,
  "editor.rulers": [
      100
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      "typescript",
      "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
}